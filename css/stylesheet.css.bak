/*style for navigation bar*/ 
.nav{
    border-color:#9370DB;
    border-width:6px ;
	background-color:#FFF0F5;
    margin:10px;
    text-align:center;
}
.nav a {
	  font-size: 20px;
	  float: left;
	  text-align: center;
	  color: #9400D3;
}
.nav a:hover {
	background-color: #DDA0DD;
	color: #FFF0F5;
}

